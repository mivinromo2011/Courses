:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x55bbd4f83d70 .scope module, "updowncounter_testbench" "updowncounter_testbench" 2 1;
 .timescale 0 0;
v0x55bbd4fcfb20_0 .var "clk", 0 0;
v0x55bbd4fcfbe0_0 .net "counter", 3 0, L_0x55bbd4fcfe80;  1 drivers
v0x55bbd4fcfcb0_0 .var "reset", 0 0;
v0x55bbd4fcfdb0_0 .var "up_down", 0 0;
S_0x55bbd4f83f00 .scope module, "dut" "up_down_counter" 2 5, 3 1 0, S_0x55bbd4f83d70;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "up_down";
    .port_info 3 /OUTPUT 4 "counter";
L_0x55bbd4fcfe80 .functor BUFZ 4, v0x55bbd4fcf810_0, C4<0000>, C4<0000>, C4<0000>;
v0x55bbd4faaa80_0 .net "clk", 0 0, v0x55bbd4fcfb20_0;  1 drivers
v0x55bbd4fcf730_0 .net "counter", 3 0, L_0x55bbd4fcfe80;  alias, 1 drivers
v0x55bbd4fcf810_0 .var "counter_up_down", 3 0;
v0x55bbd4fcf8d0_0 .net "reset", 0 0, v0x55bbd4fcfcb0_0;  1 drivers
v0x55bbd4fcf990_0 .net "up_down", 0 0, v0x55bbd4fcfdb0_0;  1 drivers
E_0x55bbd4fbc320 .event posedge, v0x55bbd4fcf8d0_0, v0x55bbd4faaa80_0;
    .scope S_0x55bbd4f83f00;
T_0 ;
    %wait E_0x55bbd4fbc320;
    %load/vec4 v0x55bbd4fcf8d0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55bbd4fcf810_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55bbd4fcf990_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x55bbd4fcf810_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55bbd4fcf810_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x55bbd4fcf810_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x55bbd4fcf810_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55bbd4f83d70;
T_1 ;
    %vpi_call 2 7 "$monitor", "%b,  %b,  %b, %b", v0x55bbd4fcfb20_0, v0x55bbd4fcfcb0_0, v0x55bbd4fcfdb0_0, v0x55bbd4fcfbe0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55bbd4fcfb20_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x55bbd4fcfb20_0;
    %inv;
    %store/vec4 v0x55bbd4fcfb20_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x55bbd4f83d70;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55bbd4fcfcb0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55bbd4fcfdb0_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55bbd4fcfcb0_0, 0, 1;
    %delay 200, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55bbd4fcfdb0_0, 0, 1;
    %end;
    .thread T_2;
